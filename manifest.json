{
  "manifest_version": 2,
  "name": "Font Finder Extension",
  "description": "A simple extension to search for font types",
  "version": "1.0",
  "developer": "mmirzafahlefi",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "js": ["fontFinder.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
